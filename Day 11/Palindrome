class Palindrome
{
public static void main(String arg[])
{
String str = "ABAB";
String rev="";
char ch;
int n = str.length();
for(int i=n-1;i>0;i--)
{
ch = str.charAt(i);
rev=rev+ch;
}
if(str.equals(rev))
{
System.out.println("is palindrome" );
}
else {
System.out.println("is not  palindrome" );
}
}
}
